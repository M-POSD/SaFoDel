<?xml version="1.0" encoding="utf-8"?>

<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/homeFragmentXML"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    tools:context=".fragment.menuBottom.HomeFragment">


    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/homeCoordinatorLayout1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true">


        <include
            android:id="@+id/home_page_images"
            layout="@layout/homepage_images" />

        <include
            android:id="@+id/toolbar"
            layout="@layout/simple_toolbar"
            app:layout_collapseMode="pin" />


        <androidx.core.widget.NestedScrollView
            android:id="@+id/home_scroll_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="75dp"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">

            <include
                android:id="@+id/homepage_button_layout"
                layout="@layout/homepage_button_layout" />

        </androidx.core.widget.NestedScrollView>

    </androidx.coordinatorlayout.widget.CoordinatorLayout>

    <FrameLayout
        android:id="@+id/container"
        android:layout_width="0dp"
        android:layout_height="0dp" />
</androidx.coordinatorlayout.widget.CoordinatorLayout>
